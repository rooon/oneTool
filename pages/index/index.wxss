/* .clearfix{
  *zoom: 1;
} */
.clearfix:before, 
.clearfix:after{
  content:"";
  display:table;
  height:0;
  visibility:hidden;
  clear:both;
}
.index{
	position: relative;
	width: 100%;
	height: auto;
	font-family: "Microsoft YaHei";
  background:#f5f6f7;
}
.editIndex{
	width: 100%;
	height: 100%;
}
.editIndex .h1{
  font-family: "Microsoft YaHei";
  color: #333;
  min-height: 40rpx;
  font-size: 35rpx;
  background: #fff;
  padding:15rpx 30rpx;
}
.info{
	background: #fff;
	height: 130rpx;
	padding:15rpx 30rpx;
	padding-top:0;
	margin-bottom: 20rpx;
}
.info .button{
  padding:20rpx;
	height: 60rpx;
  line-height: 25rpx;
	width: 200rpx;
	font-size:22rpx;
}
.info .infohd{
  position: relative;
	float: left;
	width: 120rpx;
	height: 120rpx;
	border:1rpx solid #aaa;
	overflow:hidden;
}
.info .infoname{
  float: left;
	padding:20rpx;
	height: 20rpx;
  line-height: 20rpx;
	width: 200rpx;
	font-size:32rpx;
	color:#333;
	border:2rpx solid #aaa;
  margin-left:20rpx;
}
.editIndex .editbox{
	width: 100%;
	min-height:300rpx;
	background: #fff;
	border-bottom: 1rpx solid #aaa;
	margin-bottom:50rpx;
}
.editbox textarea{
	width: 92%;
	outline: none;
	height: 120rpx;
	border: none;
	font-size: 28rpx;
	font-family: "Microsoft YaHei";
	color: #404040;
	padding: 30rpx 4%;
}
.editbox .upimg{
	padding:30rpx 30rpx;
	min-height: 120rpx;
}
.upimg .imgList{
	position: relative;
	width:118rpx;
	height: 118rpx;
	float: left;
	margin-right: 20rpx;
	margin-bottom: 30rpx
}
.upimg .imgList:last-child{
  margin-right: 0;
}
.upimg .imgList .img{
  width:118rpx;
  height:118rpx;
}
.delimg{
	position: absolute;
	right:-15rpx;
	top:-15rpx;
	display: inline-block;
	width: 30rpx;
	height: 30rpx;
	background: #fff url(data:image/png;base64,) no-repeat center;
	border-radius: 50%;
	z-index: 888;
  background-size:40rpx 40rpx;
}
.upimg .imgList:nth-child(5){
	margin-right: 0
}
.imgList img{
	position: relative;
	z-index: 2;
	width: 120px;
	height: 120px;
}
.upimg .upbtn{
	position: relative;
	float: left;
	width: 118rpx;
	height: 118rpx;
	border:1px solid #aaa;
	overflow:hidden;
}
.upbtn .span{
	position: absolute;
	display: inline-block;
  background: #ddd;
}
.upbtn .span:first-child{
	width: 90rpx;
	height: 4rpx;
	top:58rpx;
	left:15rpx;
}
.upbtn .span:last-child{
	width: 4rpx;
	height: 90rpx;
	left:58rpx;
	top:15rpx;
}
.upbtn input{
	display: inline-block;
	position: absolute;
	top: -58rpx;
	left: -15rpx;
	opacity: 0;
	width: 118rpx;
	height: 118rpx;
}
.options{
	background: #fff;
	border-top:1rpx solid #aaa;
	padding:30rpx;
}
.options .list{
	width: 100%;
	min-height: 40rpx;
	margin-bottom: 35rpx; 
}
.options .list:last-child{
	margin-bottom: 0;
}
.list .listname{
	float: left;
	font-family: "Microsoft YaHei";
	color:#333;
	min-height: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
	font-size:28rpx;
}
.list .listbox{
  position: relative;
	padding-left:265rpx;
	min-height: 80rpx;
  height:80rpx;
  line-height: 80rpx;
} 
.listbox .range{
  position: relative;
  top:4rpx;
  width:400rpx;
  font-size: 36rpx;
}   
.listcont{
  width: 92%;
  min-height: 242rpx;
  padding-top: 80rpx;
  margin: 0 auto;
}
.listcont .select,
.listcont label{
  float: left;
  width: 190rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #666;
  font-family: "Microsoft YaHei";
  margin: 10rpx 10rpx 30rpx 10rpx;
}
.listcont label{
  margin:0;
}
.switch {
  position: relative;
	float:right;
  top:-5rpx;
}
.comment,
.location{
	width: 97%;
	/* padding-top:30rpx; */
	min-height: 160rpx;
	margin:0 auto;
}
.location{
  min-height: 180rpx;
}
.comment .div{
  position: relative;
	font-size: 28rpx;
	height: 80rpx;
	line-height: 80rpx;
	margin-bottom: 40rpx;
}
.comment .commentrange{
  position: absolute;
  right:-10rpx;
  top:-16rpx;
  width:480rpx;
}
.comment input[type = "text"]{
  position: absolute;
  top:14rpx;
  right: 20rpx;
	border:1rpx solid #aaa;
	height:16px;
	width: 460rpx;
	padding:5rpx 20rpx;
	outline: none;
	font-size:24rpx;
}
.location .btn{
	margin:20rpx auto;
	height: 16rpx;
}
.location .btn .span{
  float:left;
	display: inline-block;
	padding:15rpx;
	height: 35rpx;
	line-height: 35rpx;
	border:1rpx solid #aaa;
	margin-right:35rpx;
	width: 240rpx;
	text-align: center;
	font-size:30rpx;
  margin-left:10rpx;
}
.location .btn .span:last-child{
  margin-right: 0;
}
.location .btn .span.current{
  background:orange;
  border-color:orange;
  color:#fff;
}
.location input{
  float:left;
  height:40rpx;
  padding:10rpx 20rpx;
  font-size:26rpx;
  outline:none;
  border:1rpx solid #aaa;
  width:544rpx;
  margin-left:10rpx;
  margin-top:20rpx;
}
.showlocation{
	height: 96rpx;
}
.generate{
	height: 110rpx;
	width: 650rpx;
	text-align: center;
	font-family: "Microsoft YaHei";
	font-size: 50rpx;
	line-height: 110rpx;
	cursor: pointer;
	border-radius: 8rpx;
	background: #09BB07;
	color:#fff;
	margin:40rpx auto;
}

.showIndex{
	position: relative;
	width: 750rpx;
	min-height:1200rpx;
}
.showIndex .hd{
	height: 92rpx;
	padding:0 30rpx;
	background: #393a3f;
}
.hd .back{
	position: relative;
	float:left;
	width:99rpx;
	height:46rpx;
	line-height: 46rpx;
	background: url(data:image/png;base64,) no-repeat left center;
  background-size: 33rpx 31rpx;
	padding-left:116rpx;
	margin-top:23rpx;
	color:#fff;
	font-size:28rpx;
}
.back em{
  position: absolute;
  display: inline-block;
  width: 2rpx;
  height: 46rpx;
  background: #2d2d31;
  left:80rpx;
}
.hd .camera{
	float: right;
	display: inline-block;
	width: 45rpx;
	height: 35rpx;
	margin-top:28rpx;
	background: url(data:image/png;base64,) no-repeat center;
background-size: 45rpx 35rpx;
}
.main{
	width: 100%;
	height: auto;
}
.main .hdbg{
	position: relative;
	width: 100%;
	height: 512rpx;
}
.hdbg .img{
	width: 100%;
	height: 512rpx;
}
.hdbg .hdimg{
	position: absolute;
	right:23rpx;
	bottom:-63rpx;
	width: 160rpx;
	height: 160rpx;
	border:1rpx solid #FFF;
}
.hdbg .hdimg .img{
	width: 100%;
	height: 100%;
}
.hdbg .name{
	position: absolute;
	right: 206rpx;
	bottom:23rpx;
	font-size: 28rpx;
	color: #fff;
}
.content{
	padding:0 23rpx;
	padding-top:95rpx;
}
.showIndex .list{
	width: 100%;
	min-height: 80rpx;
	border-bottom: 1rpx solid #bfbfbf;
	padding-bottom: 20rpx;
	margin-bottom: 25rpx;
}
.list .listLeft{
	margin-right: 16rpx;
}
.list .listRight{
	width: 605rpx;
	height: auto;
}
.list .listLeft,
.list .listRight{
	float:left;
}
.listLeft .hdimg,
.hdimg .img{
	width: 80rpx;
	height: 80rpx;
}
.list .name{
	color:#294278;
	font-size: 28rpx;
}
.desc{
	width:100%;
	height: auto;
}
.list .word{
	margin:16rpx 0 26rpx 0;
	font-size:27rpx;
	color:#303030;
}
.list .pic{
	width: 565rpx;
	min-height: 185rpx;
}
.list .one .img{
	max-width: 60%;
}
.list .more .img{
	float: left;
	width: 184rpx;
	height: 180rpx;
	margin:0 5rpx 5rpx 0;
}
.list .more .img:nth-child(3n){
	margin-right:0;
}
.list .wz{
	color:#838383;
	font-size: 25rpx;
	margin-top:15rpx;
}
.list .qt{
	margin-top:10rpx;
	margin-bottom: 25rpx;
}
.qt .span:first-child{
	color:#838383;
	font-size: 25rpx;
}
.qt .span:last-child{
  position: relative;
  float: right;
  display: inline-block;
  width: 43rpx;
  top: 17rpx;
  height: 31rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 43rpx 31rpx;
}
.gc{
	position: relative;
	width: 100%;
	height: auto;
	background: #eaeaea;
}
.gc::before{
	position: absolute;
	content: '';
	left: 17rpx;
	top: -10rpx;
	width: 0;
	height: 0;
	border-left: 13rpx solid transparent;
	border-right: 13rpx solid transparent;
	border-bottom: 12rpx solid #eaeaea;
}
.gc .GiveList{
	padding:20rpx;
}
.GiveList .p{
	color:#3d5586;
	font-size: 28rpx;
}
.GiveList .d{
	color:#3d5586;
	font-size: 28rpx;
	margin-right: 4rpx;
	margin-left:2rpx;
}
.GiveList .p:last-child .d{
	display: none;
}
.GiveList .xin{
  position: relative;
  top: 3rpx;
  display: inline-block;
  width: 25rpx;
  height: 25rpx;
  background: url(data:image/png;base64,) no-repeat center;
  background-size: 25rpx 25rpx;
}
.commentList{
	border-top: 1rpx solid #bfbfbf;
	padding: 18rpx 13rpx 18rpx 13rpx;
}
.commentList .p .span{
	color:#3d5586;
	font-size: 28rpx;
}
.commentList .p{
	font-size: 25rpx;
	color:#3C3C3C;
	margin-bottom: 8rpx;
}
.savepic{
  position: fixed;
  bottom: 40rpx;
  right: 40rpx;
  width: 100rpx;
  height: 100rpx;
  background: green;
  color: #fff;
  font-size: 70rpx;
  text-align: center;
  line-height: 87rpx;
  z-index: 99;
  border-radius: 50%;
}